@model IEnumerable<Lab10.trip>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @if (User.Identity.IsAuthenticated)
    {
        @Html.ActionLink("Create New", "Create")
    }
    </p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model._object.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.car.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.object1.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date_from)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date_to)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item._object.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.car.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.object1.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.date_from)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.date_to)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.time)
    </td>
    @if (User.Identity.IsAuthenticated)
    {
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
            @Html.ActionLink("Details", "Details", new { id = item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.id })
        </td>
    }
    </tr>
}

</table>
